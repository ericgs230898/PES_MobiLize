    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".optionsActivity"></activity>
        <activity
            android:name=".PopUpRutina"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".Seleccionar_rutina" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ModalitatExercici" />
        <activity android:name=".ModalitatEntrenament" />
        <activity android:name=".NivellEntrenament" />
        <activity android:name=".RegisterMessageActivity" />
